{
    "manifest_version": 2,
    "name": "TabBehavior",
    "version": "0.1.2",
    "description": "Changes tab opening and closing behavior.",
    "homepage_url": "https://github.com/mekras/TabBehavior",
    "applications": {
        "gecko": {
            "id": "tab-behavior@vsepofigu.ru",
            "strict_min_version": "57.0"
        }
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "main.js"
        ]
    },
    "icons": {
        "32": "assets/icons/tab-behavior-32.png",
        "48": "assets/icons/tab-behavior-48.png"
    }
}
